{
  "name": "hottransilvania-monorepo",
  "version": "1.0.0",
  "description": "Monorepo para Hotel Transilvania - Backend e Frontend",
  "private": true,
  "workspaces": [
    "Backend",
    "Frontend"
  ],
  "scripts": {
    "install:all": "npm install && cd Backend && npm install && cd ../Frontend && npm install",
    "dev:backend": "cd Backend && npm run dev",
    "dev:frontend": "cd Frontend && npm run dev",
    "build:frontend": "cd Frontend && npm run build",
    "start:backend": "cd Backend && npm start"
  },
  "keywords": [
    "monorepo",
    "hotel",
    "transilvania"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}

